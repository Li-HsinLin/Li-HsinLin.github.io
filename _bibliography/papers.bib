@article{Working,
  abbr={Working},
  title={A License to Lie: An Insurance and Reporting Experiment},
  author={Lin, L. H.},
  abstract={Markets with asymmetric information are typically characterized by the dual challenges of adverse selection and moral hazard, yet their interaction remains underexplored in contexts that do not rely on strong assumptions. This study presents a novel experimental framework that varies participants’ awareness of their ability to misreport losses to examine whether such awareness influences their willingness to pay (WTP) for insurance. The design isolates two empirically indistinguishable pathways of dishonest behavior: (1) Ex post moral hazard, where individuals become aware of dishonest opportunities only after purchasing insurance, and (2) Premeditated dishonesty (adverse selection on moral hazard), where individuals anticipate the potential for dishonesty before making their insurance decision and adjust their WTP accordingly. Experimental results show that explicit permission to misreport increases maximum claims from 7.84\% to 41.86\% and 45.28\%, while advance knowledge of this option raises maximum WTP from 7.84\% and 4.65\% to 20.75\%. Interestingly, participants with higher WTP in this treatment filed smaller claims, suggesting advantageous selection on moral hazard that insurers may leverage when verification costs are high.},
